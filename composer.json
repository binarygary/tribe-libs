{
    "name": "moderntribe/tribe-libs",
    "description": "A library for use on Modern Tribe service projects.",
    "license": "GPL-2.0-only",
    "require": {
        "php": "^7.2",
        "filp/whoops": "^2.2@dev",
        "php-di/php-di": "^6.0",
        "twig/twig": "^3.0",
        "moderntribe/square1-acf": "dev-master",
        "moderntribe/square1-assets": "dev-master",
        "moderntribe/square1-blog-copier": "dev-master",
        "moderntribe/square1-cache": "dev-master",
        "moderntribe/square1-cli": "dev-master",
        "moderntribe/square1-container": "dev-master",
        "moderntribe/square1-generators": "dev-master",
        "moderntribe/square1-media": "dev-master",
        "moderntribe/square1-nav": "dev-master",
        "moderntribe/square1-object-meta": "dev-master",
        "moderntribe/square1-oembed": "dev-master",
        "moderntribe/square1-p2p": "dev-master",
        "moderntribe/square1-post-meta": "dev-master",
        "moderntribe/square1-post-type": "dev-master",
        "moderntribe/square1-queues": "dev-master",
        "moderntribe/square1-queues-mysql": "dev-master",
        "moderntribe/square1-required-page": "dev-master",
        "moderntribe/square1-schema": "dev-master",
        "moderntribe/square1-settings": "dev-master",
        "moderntribe/square1-taxonomy": "dev-master",
        "moderntribe/square1-user": "dev-master",
        "moderntribe/square1-utils": "dev-master",
        "moderntribe/square1-whoops": "dev-master"
    },
    "require-dev": {
        "automattic/phpcs-neutron-standard": "^1.5",
        "automattic/vipwpcs": "^2.0.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "lucatume/wp-browser": "^2.6.10",
        "phpcompatibility/php-compatibility": "*",
        "phpcompatibility/phpcompatibility-wp": "^2.0",
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^8.0",
        "sirbrillig/phpcs-variable-analysis": "^2.0.1",
        "squizlabs/php_codesniffer": "^3.4.2",
        "symplify/monorepo-builder": "^8.2",
        "szepeviktor/phpstan-wordpress": "^0.6.0",
        "wp-cli/wp-cli": "^2.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Tribe\\Libs\\": "tests/integration/Tribe/Libs"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "platform": {
            "php": "7.2.32"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
